{
    "server": {
        "app": {
            "name": "testApp",
            "nested": {

            },
            "nameCopy": "config:server.app.name"
        },
        "debug": {
            "log": {
                "$filter": "env.NODE_ENV",
                "$default": ["debug"],
                "production": ["warn"]
            }
        }
    },
    "register": {
        "devPlugin": {
            "plugin": {
                "$filter": "env.NODE_ENV",
                "$default": "require:../devPlugin.js",
                "production": "require:../plugin.js"
            }
        },
        "otherPlugin": {
            "plugin": "require:../otherPlugin.js",
            "before": "devPlugin"
        }
    },
    "routes": [
        {
            "path": "/admin",
            "method": "GET",
            "handler": "require:../admin"
        }
    ]
}
